{
  "ReverseProxy": {
    "Routes": {
      "estoqueRoute": {
        "ClusterId": "estoqueCluster",
        "Match": {
          "Path": "/estoque/{**catch-all}"
        }
      },
      "vendasRoute": {
        "ClusterId": "vendasCluster",
        "Match": {
          "Path": "/vendas/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "estoqueCluster": {
        "Destinations": {
          "dest1": {
            "Address": "http://localhost:5185/"
          }
        }
      },
      "vendasCluster": {
        "Destinations": {
          "dest1": {
            "Address": "http://localhost:5242/"
          }
        }
      }
    }
  }
}
