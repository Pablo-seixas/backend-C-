{
  "ReverseProxy": {
    "Routes": {
      "produtosRoute": {
        "ClusterId": "estoqueCluster",
        "Match": {
          "Path": "/api/produto/{**catch-all}"
        }
      },
      "vendasRoute": {
        "ClusterId": "vendasCluster",
        "Match": {
          "Path": "/api/vendas/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "estoqueCluster": {
        "Destinations": {
          "dest1": {
            "Address": "http://localhost:5185/"
          }
        }
      },
      "vendasCluster": {
        "Destinations": {
          "dest1": {
            "Address": "http://localhost:5242/"
          }
        }
      }
    }
  }
}
